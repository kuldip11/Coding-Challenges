import java.util.Scanner;
 
public class theatre {
  public static void main(String[] args) {
		 double n,m,a;
		 Scanner in = new Scanner(System.in);
 
		     
		      n =  in.nextDouble();   
		      m = in.nextDouble();   
		      a = in.nextDouble();   
		      System.out.println((long)Math.ceil(n/a)*(long)Math.ceil(m/a) );
		      
		 }
 
}
